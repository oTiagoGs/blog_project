<div class="row">
  <div class="col-lg-12">
     <h1 class="page-header">Listando os Administradores</h1>
  </div>
</div>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th>#</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
          <% @admins.each do |admin| %>
                <tr>
                    <td><%= admin.id %></td>
                    <td><%= admin.email %></td>
                </tr>
          <% end %>
        </tbody>
    </table>
</div>